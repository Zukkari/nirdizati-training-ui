<?xml version="1.0" encoding="UTF-8" ?>

<configuration>
    <pageConfig>
        <page>
            <id>landing</id>
            <uri>/views/landing.zul</uri>
        </page>
        <page>
            <id>uploadLog</id>
            <uri>/views/upload_page.zul</uri>
        </page>
        <page>
            <id>logOverview</id>
            <uri>/views/logOverview.zul</uri>
        </page>
        <page>
            <id>training</id>
            <uri>/views/training.zul</uri>
        </page>
        <page>
            <id>validation</id>
            <uri>/views/validation.zul</uri>
        </page>
    </pageConfig>
    <userLogDirectory>../userlogs/</userLogDirectory>

    <headerConfiguration>
        <headerItem>
            <label>header.upload</label>
            <redirect>uploadLog</redirect>
            <enabled>true</enabled>
            <position>0</position>
        </headerItem>
        <headerItem>
            <label>header.overview</label>
            <redirect>logOverview</redirect>
            <enabled>false</enabled>
            <position>1</position>
        </headerItem>
        <headerItem>
            <label>header.training</label>
            <redirect>training</redirect>
            <enabled>true</enabled>
            <position>2</position>
        </headerItem>
        <headerItem>
            <label>header.validation</label>
            <redirect>validation</redirect>
            <enabled>false</enabled>
            <position>3</position>
        </headerItem>
    </headerConfiguration>

    <modelConfig>
        <types>
            <type>encoding</type>
            <type>bucketing</type>
            <type>regression</type>
            <type>predictiontype</type>
        </types>
        <modelparams>
            <modelparam>
                <label>encoding.last_state</label>
                <parameter></parameter>
                <type>encoding</type>
                <enabled>true</enabled>
            </modelparam>
            <modelparam>
                <label>encoding.frequency</label>
                <parameter></parameter>
                <type>encoding</type>
                <enabled>true</enabled>
            </modelparam>
            <modelparam>
                <label>encoding.combined</label>
                <parameter></parameter>
                <type>encoding</type>
                <enabled>true</enabled>
            </modelparam>
            <modelparam>
                <label>encoding.index_based</label>
                <parameter></parameter>
                <type>encoding</type>
                <enabled>true</enabled>
            </modelparam>
            <modelparam>
                <label>bucketing.single</label>
                <parameter></parameter>
                <type>bucketing</type>
                <enabled>true</enabled>
            </modelparam>
            <modelparam>
                <label>bucketing.state_based</label>
                <parameter></parameter>
                <type>bucketing</type>
                <enabled>true</enabled>
            </modelparam>
            <modelparam>
                <label>bucketing.clustering</label>
                <parameter></parameter>
                <type>bucketing</type>
                <enabled>true</enabled>
            </modelparam>
            <modelparam>
                <label>bucketing.prefix_lenght_based</label>
                <parameter></parameter>
                <type>bucketing</type>
                <enabled>true</enabled>
            </modelparam>
            <modelparam>
                <label>regr.random_forest</label>
                <parameter></parameter>
                <type>regression</type>
                <enabled>true</enabled>
            </modelparam>
            <modelparam>
                <label>regr.gradient_boosting</label>
                <parameter></parameter>
                <type>regression</type>
                <enabled>true</enabled>
            </modelparam>
            <modelparam>
                <label>regr.decision_tree</label>
                <parameter></parameter>
                <type>regression</type>
                <enabled>true</enabled>
            </modelparam>

            <modelparam>
                <label>prediction.remaining_time</label>
                <parameter></parameter>
                <type>predictiontype</type>
                <enabled>true</enabled>
            </modelparam>
            <modelparam>
                <label>prediction.outcome</label>
                <parameter></parameter>
                <type>predictiontype</type>
                <enabled>true</enabled>
            </modelparam>
        </modelparams>
    </modelConfig>
</configuration>